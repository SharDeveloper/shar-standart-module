        max(iToOM(1), iToOM(2)).typeGetItem(0) == 2
        max(iToOM(2), iToOM(1)).typeGetItem(0) == 2
        min(iToOM(0), iToOM(0)).typeGetItem(0) == 0
        min(iToOM(0), iToOM(1)).typeGetItem(0) == 0
        min(iToOM(1), iToOM(0)).typeGetItem(0) == 0
        min(iToOM(-1), iToOM(0)).typeGetItem(0) == -1
        min(iToOM(0), iToOM(-1)).typeGetItem(0) == -1
        min(iToOM(-1), iToOM(-2)).typeGetItem(0) == -2
        min(iToOM(-2), iToOM(-1)).typeGetItem(0) == -2
        min(iToOM(1), iToOM(2)).typeGetItem(0) == 1
        min(iToOM(2), iToOM(1)).typeGetItem(0) == 1

