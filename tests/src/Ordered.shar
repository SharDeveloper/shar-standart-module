        !iToOM(22).inRange?(iToOM(23), iToOM(22))
        max(iToOM(0), iToOM(0)).typeGetItem(0) == 0
        max(iToOM(0), iToOM(1)).typeGetItem(0) == 1
        max(iToOM(1), iToOM(0)).typeGetItem(0) == 1
        max(iToOM(-1), iToOM(0)).typeGetItem(0) == 0
