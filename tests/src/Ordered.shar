        !iToOM(0).inRange?(iToOM(1), iToOM(1))
        !iToOM(0).inRange?(iToOM(1), iToOM(-1))
        !iToOM(0).inRange?(iToOM(1), iToOM(0))
        iToOM(22).inRange?(iToOM(0), iToOM(100))
        iToOM(22).inRange?(iToOM(22), iToOM(23))
        !iToOM(22).inRange?(iToOM(21), iToOM(22))
        !iToOM(22).inRange?(iToOM(22), iToOM(22))
