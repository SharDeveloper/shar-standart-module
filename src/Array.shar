            const secondItem $Any = array[smallRangeLeftIndex + 1]
            if (secondItem < firstItem) == rise
                nothrow
                    array.swap(smallRangeLeftIndex, smallRangeLeftIndex + 1)
        if bigRangeLeftIndex == -1
