        const rangeLength Int = rightEdge - leftIndex
        var smallRangeLeftIndex Int = -1
        var smallRangeRightEdge Int = -1
        var smallRangeLength Int = -1
        var bigRangeLeftIndex Int = -1
        var bigRangeRightEdge Int = -1
